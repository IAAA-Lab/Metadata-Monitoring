#################################################################################
#### Completeness  ###

:DQCompleteness a dqv:Dimension ;
	dqv:inCategory ldqd:intrinsicDimensions ;
    skos:prefLabel "DQ_Completeness"@en;
    skos:definition "Presence or absence of metadata elements."@en .

#--------------------------------------

:DQCompletenessCommission skos:broader :DQCompleteness;
    skos:prefLabel "DQ_CompletenessCommission"@en;
    skos:definition "Checking for excess instances of metadata elements in a metadata record."@en .

:D.3.ISO.19157 a dqv:Metric ;
    skos:definition "Rate of records with excess items."@en ;
    dqv:inDimension :DQCompletenessCommission;
    dqv:expectedDataType xsd:boolean.

:DQCompletenessCommission_dataset_automatic a dqv:QualityMeasurement ;
    dqv:computedOn :myCatalog ;
    dqv:isMeasurementOf :D.3.ISO.19157 ;
    dqv:value "true"^^xsd:boolean;
    dct:date "2020-03-01"^^xsd:date ;
    :onProperty dcat:dataset.

:DQCompletenessCommission_distribution_automatic a dqv:QualityMeasurement ;
    dqv:computedOn :myCatalog ;
    dqv:isMeasurementOf :D.3.ISO.19157 ;
    dqv:value "true"^^xsd:boolean;
    dct:date "2020-03-01"^^xsd:date ;
    :onProperty dcat:distribution. 

#--------------------------------------

:DQCompletenessOmission skos:broader :DQCompleteness;
    skos:prefLabel "DQ_CompletenessOmmission"@en;
    skos:definition "Checking for mandatory elements that are absent in metadata records."@en .

:D.7.ISO.19157 a dqv:Metric ;
    skos:definition "Rate of records with missing items."@en ;
    dqv:inDimension :DQCompletenessOmission;
    dqv:expectedDataType xsd:boolean.

:DQCompletenessOmission_dataset_automatic a dqv:QualityMeasurement ;
    dqv:computedOn :myCatalog ;
    dqv:isMeasurementOf :D.7.ISO.19157 ;
    dqv:value "true"^^xsd:boolean;
    dct:date "2020-03-01"^^xsd:date ;
    :onProperty dcat:dataset.

:DQCompletenessOmission_distribution_automatic a dqv:QualityMeasurement ;
    dqv:computedOn :myCatalog ;
    dqv:isMeasurementOf :D.7.ISO.19157 ;
    dqv:value "false"^^xsd:boolean;
    dct:date "2020-03-01"^^xsd:date ;
    :onProperty dcat:distribution.

