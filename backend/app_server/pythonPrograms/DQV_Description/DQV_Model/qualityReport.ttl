    dqv:computedOn :myCatalog ;
    dqv:isMeasurementOf :OverallQualityOfFreeText ;
    dqv:value "false"^^xsd:boolean;
    dct:date "2020-03-01"^^xsd:date ;
    :onProperty dcat:dataset, dct:title;
    :onLanguage "es".

:DQQualityOfFreeText_dataset_description_manual a dqv:QualityMeasurement ;
    dqv:computedOn :myCatalog ;
